server: https://{{ haproxy_vip }}:9345
token: {{ rke2_join_token }}
tls-san:
  - {{ haproxy_vip }}
node-ip: {{ inventory_hostname }}
node-label:
  - "dedicated={{ node_label }}"
{% if node_use_taint %}
node-taint:
  - "dedicated={{ node_label }}:NoSchedule"
{% endif %}
disable:
  - rke2-ingress-nginx
  - rke2-metrics-server
kubelet-arg:
  - "--kube-reserved=cpu=100m,memory=256Mi,ephemeral-storage=512Mi"
  - "--system-reserved=cpu={{ node_system_cpu_reserved }},memory={{ node_system_memory_reserved }}"
  - "--max-pods=91"